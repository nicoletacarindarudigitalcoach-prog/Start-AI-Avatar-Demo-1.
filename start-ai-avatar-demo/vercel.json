{
  "version": 2,
  "framework": null,
  "buildCommand": "echo \"skip build\"",
  "outputDirectory": "public",
  "routes": [
    { "src": "^/api/token$", "dest": "api/token.js" },
    { "src": "^/api/chat$", "dest": "api/chat.js" },
    { "src": "^/api/env$", "dest": "api/env.js" },
    { "src": "/(.*)", "dest": "public/$1" }
  ]
}
